{
	"name": "trakit",
	"version": "0.2.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test": "vitest run",
		"test:watch": "vitest",
		"migrate:up": "dotenv -e .env -- node-pg-migrate up",
		"migrate:down": "dotenv -e .env -- node-pg-migrate down",
		"migrate:create": "dotenv -e .env -- node-pg-migrate create",
		"generate:notification-keys": "node scripts/generate-notification-keys.mjs"
	},
	"devDependencies": {
		"@eslint/js": "^9.18.0",
		"@iconify/svelte": "^5.2.1",
		"@material/material-color-utilities": "^0.4.0",
		"@sveltejs/adapter-node": "^5.2.9",
		"@sveltejs/kit": "^2.14.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/postcss": "^4.1.18",
		"@types/luxon": "^3.7.1",
		"@types/node": "^25.0.10",
		"@types/node-cron": "^3.0.11",
		"@types/nodemailer": "^7.0.5",
		"@types/pg": "^8.11.10",
		"@types/web-push": "^3.6.4",
		"@typescript-eslint/eslint-plugin": "^8.20.0",
		"@typescript-eslint/parser": "^8.20.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^17.0.0",
		"postcss": "^8.4.49",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.3.2",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte": "^5.17.0",
		"svelte-check": "^4.1.3",
		"tailwindcss": "^4.1.18",
		"tslib": "^2.8.1",
		"typescript": "^5.7.3",
		"vite": "^7.3.1",
		"vite-plugin-pwa": "^1.2.0",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"@node-rs/argon2": "^2.0.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"dotenv-cli": "^11.0.0",
		"html2canvas": "^1.4.1",
		"luxon": "^3.7.2",
		"node-cron": "^4.2.1",
		"node-pg-migrate": "^8.0.4",
		"nodemailer": "^7.0.12",
		"pg": "^8.13.1",
		"web-push": "^3.6.7"
	},
	"overrides": {
		"cookie": "^0.7.2",
		"glob": "^11.0.4"
	}
}
